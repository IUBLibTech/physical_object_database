<table>
	<tr>
		<th>MDPI Barcode</th>
		<th>Actions</th>
	</tr>
	<tr>
		<% @bin.boxes.each do |b| %>
			<td><%= b.mdpi_barcode %></td>
			<td>
				<%= link_to("Show", action: 'show_box', id: b.id) %>
				<%= link_to("Remove", action: 'remove_box', id: b.id) %>
				<%= link_to("Edit", action: 'edit_box', id: b.id) %>
			</td>	
		<% end %>
	</tr>
</table>