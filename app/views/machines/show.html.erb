<% content_for :context_menu do %>
  <div class="contextMenuItem">
    <%= link_to "Edit", edit_machine_path(@machine) %>
  </div>
  <div class="contextMenuItem">
  <%= link_to "Delete", machine_path(@machine), method: :delete, data: { confirm: 'Are you sure you want to delete this Machine?' } %>
  </div>
  <div class="contextMenuItem">
    <%= link_to "Machine Index", machines_path %>
  </div>
<% end %>

<%= render partial: 'machine' %>

