<% content_for :context_menu do %>
	<div class="contextMenuItem">
    <%= link_to("Edit", :action => 'edit', id: @picklist.id) %>
  </div>
  <div class="contextMenuItem">
		<%= link_to("Delete", picklist_path(@picklist.id), method: :delete, data: {confirm: 'Are you sure you want to delete this Picklist?'}) %>
  </div>
<% end %>
<%= render(:partial => "picklist") %>
