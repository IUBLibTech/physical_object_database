<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:type="String">Picklist:</Data></Cell>
        <Cell><Data ss:type="String"><%= @picklist.name %></Data></Cell>
      </Row>
      <Row>
      <% @column_names = PhysicalObject.printable_columns %>
      <% @column_names.each do |column_name| %>
        <Cell><Data ss:Type="String"><%= column_name.titleize %></Data></Cell>
      <% end %>
      <% if @physical_objects.any? %>
        <% @md_columns = @physical_objects.first.metadata_columns %>
        <% @md_columns.each do |md_column| %>
          <Cell><Data ss:Type="String"><%= md_column.titleize %></Data></Cell>
        <% end %>
      <% end %>
      </Row>
    <% @physical_objects.each do |po| %>
      <Row>
        <% po.printable_row.each do |cell_value| %>
          <Cell><Data ss:Type="String"><%= cell_value %></Data></Cell>
	<% end %>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
