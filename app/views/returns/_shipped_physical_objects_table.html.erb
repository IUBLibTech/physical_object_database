<% @shipped.each do |p| %>
	<%= form_for(p, url: {controller: 'returns', action: "physical_object_missing", id: p.id}) do |f| %>
		<table class="picklistTable">
				<tr class="odd packed">
					<td class='ephemera'>
						<% if p.has_ephemera %>
							<%= image_tag "folder.png", {alt: 'had ephemera'} %>
						<% end %>
					</td>
					<td>
							<%= image_tag "gs_mdpi_barcode.png", {alt: "mdpi barcode"} %>
							<%= f.text_field(:mdpi_barcode, readonly: true, maxlength: "14", size: "14", class: "mdpi") %>
					</td>
					<td>
						<%= link_to(physical_object_path(p.id), target: "_blank") do %>
							Call Number: <%= p.call_number %>
						<% end %>
					</td>
				</tr>
		</table>
	<% end %>
<% end %>
